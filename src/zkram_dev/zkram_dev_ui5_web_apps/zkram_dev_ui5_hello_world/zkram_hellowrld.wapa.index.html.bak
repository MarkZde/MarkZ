<!DOCTYPE html>                                                                                                                                                                                                                                                
<html>                                                                                                                                                                                                                                                         
	<head>                                                                                                                                                                                                                                                        
		<meta charset="utf-8">                                                                                                                                                                                                                                       
        <meta name="viewport" content="width=device-width, initial-scale=1.0">                                                                                                                                                                                 
        <link href="img/markzde.ico" rel="shortcut icon" type="image/x-icon" />                                                                                                                                                                                
        <title>MarkZ</title>                                                                                                                                                                                                                                   
        <!-- ./resources/sap-ui-core.js -->                                                                                                                                                                                                                    
        <!-- https://sapui5.hana.ondemand.com/1.52.20/resources/sap-ui-core.js -->                                                                                                                                                                             
		<script id="sap-ui-bootstrap"                                                                                                                                                                                                                                
                src="./resources/sap-ui-core.js"                                                                                                                                                                                                               
                data-sap-ui-theme="sap_belize_plus"                                                                                                                                                                                                            
                data-sap-ui-libs="sap.m"                                                                                                                                                                                                                       
			    data-sap-ui-resourceroots='{"com.zkram.helloworld": "./"}'                                                                                                                                                                                              
			    data-sap-ui-compatVersion="edge"                                                                                                                                                                                                                        
                data-sap-ui-preload="async"                                                                                                                                                                                                                    
                data-sap-ui-frameOptions="trusted">                                                                                                                                                                                                            
        </script>                                                                                                                                                                                                                                              
        <!-- data-sap-ui-async="true" -->                                                                                                                                                                                                                      
        <!-- data-sap-ui-oninit="module:sap/ui/core/ComponentSupport" -->                                                                                                                                                                                      
        <script>                                                                                                                                                                                                                                               
		    sap.ui.getCore().attachInit(function () {                                                                                                                                                                                                                
			    new sap.m.Shell({                                                                                                                                                                                                                                       
				    app : new sap.ui.core.ComponentContainer({                                                                                                                                                                                                             
					    name : "com.zkram.helloworld",                                                                                                                                                                                                                        
					    settings : {                                                                                                                                                                                                                                          
						    id : "helloworld"                                                                                                                                                                                                                                    
					    }                                                                                                                                                                                                                                                     
				    })                                                                                                                                                                                                                                                     
			    }).placeAt("content");                                                                                                                                                                                                                                  
		    });                                                                                                                                                                                                                                                      
	    </script>                                                                                                                                                                                                                                                 
	</head>                                                                                                                                                                                                                                                       
	<body class="sapUiBody" id="content">                                                                                                                                                                                                                         
        <div data-sap-ui-component data-name="com.zkram.helloworld" data-id="container" data-settings='{"id" : "helloworld"}'></div>                                                                                                                           
	</body>                                                                                                                                                                                                                                                       
</html>                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               